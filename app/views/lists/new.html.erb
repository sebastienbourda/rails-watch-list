<%# New List View %>

<div class="container-fluid">
  <%= simple_form_for @list do |f| %>
    <%= f.error_notification %>

    <%= f.input :name %>
    <%= f.button :submit, class: "btn-primary" %>
    <%= f.button :button, "Cancel", type: "reset", class: "btn-outline-secondary" %>
  <% end %>
</div>
